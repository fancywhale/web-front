{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom", 
      "es7"
    ],
    "types": [
      "reflect-metadata", 
      "jquery"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "experimentalDecorators":true,
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "declaration": false,
    "allowJs": true
  },
  "exclude": [
    "node_modules",
    "build"
  ]
}
